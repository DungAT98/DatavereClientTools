<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>net462;net472;net48;netcoreapp3.1</TargetFrameworks>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>Dataverse Client Tools</Title>
        <Authors>DungAT</Authors>
        <Description>This is some functions to help developers to work with Microsoft.Dataverse.Client</Description>
        <Copyright>DungAT</Copyright>
        <PackageProjectUrl>https://github.com/DungAT98/DatavereClientTools</PackageProjectUrl>
        <RepositoryUrl>https://github.com/DungAT98/DatavereClientTools</RepositoryUrl>
        <PackageTags>dataverse,microsoft,dynamics,crm,dynamic365</PackageTags>
        <AssemblyVersion>1.0.0</AssemblyVersion>
        <FileVersion>1.0.0</FileVersion>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.PowerPlatform.Dataverse.Client" Version="1.0.1" />
    </ItemGroup>

</Project>
